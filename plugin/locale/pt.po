msgstr " <vazio>"

msgid ""
"%s : Switch to channel list\n"
"Ok key : Remove selected channel\n"
"PVR key : Input channel name\n"
"Exit key : Finish channel edit"
msgstr ""
"%s : Mudar para a lista de canais\n"
"Botão Ok : Remover o canal selecionado\n"
"Botão PVR : Introduzir nome do canal\n"
"Botão Exit : Terminar a edição da lista de canais"

msgid ""
"%s : Switch to quad PiP entry\n"
"Ok key : Add to new entry\n"
"PVR key : Input channel name\n"
"Exit key : Finish channel edit"
msgstr ""
"%s : Mudar para a entrada Quad PiP.\n"
"Botão Ok : Adicionar uma nova entrada\n"
"Botão PVR : Introduzir nome do canal\n"
"Botão Exit : Terminar a edição de canal"

